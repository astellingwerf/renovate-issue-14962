{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "astellingwerf/renovate-reproduction"
      ],
      "versioning": "regex:^(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)(-(?<compatibility>.*))?$"
    },
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackageNames": [
        "astellingwerf/renovate-reproduction"
      ],
      "replacementName": "astellingwerf/renovate-reproduction",
      "matchCurrentVersion": "/^1\\.0\\.0-flavour1$/",
      "replacementVersion": "1.0.0-flavour2",
      "additionalBranchPrefix": "{{currentVersion}}",
    },
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackageNames": [
        "astellingwerf/renovate-reproduction"
      ],
      "replacementName": "astellingwerf/renovate-reproduction",
      "matchCurrentVersion": "/^1\\.0\\.0-flavour1-suffix$/",
      "replacementVersion": "1.0.0-flavour2-suffix",
      "additionalBranchPrefix": "{{currentVersion}}"
    }
  ]
}
